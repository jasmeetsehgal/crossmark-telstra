@{
    ViewBag.Title = "About Us";
}

<div class="AlignH cf">
<div class="sub-page">
    <a class="scrollable-browse scrollable-next"></a>
    <a class="scrollable-browse scrollable-prev"></a>
    <div class="title">
        <h1>@ViewBag.Title</h1>
    </div>
    <div>
        <div class="items">
            <div class="page-content">
                
                <div class="scrollable_wrap"><img src="/Content/img/aboutus/cretingdigital_img.jpg" class="creatingdigital_img" />
                    <h1>ABOUT TELSTRA DIGITAL</h1>
                    <div class="video-container">
                        <video id="videoplayer" autoplay class="about-videos"  controls="controls">
                            <source src="/Content/videos/About%20TD.mp4" type="video/mp4">
                        </video>
                    </div>
                </div>
            </div>
            @*end item here*@
        </div>
    </div>
</div>

<div class="video-thumbs AlignV">
    <script type="text/javascript">
        window.parent.mainSoundPause(true);
</script>
   <a href="/Content/videos/About%20TD.mp4" data-title="About Telstra Digital" class="btn active">
